1.Linear Search:
(Seq. search)
#include<iostream>
using namespace std;

int main()
{
    int n,k,i,found=0;
    cin >> n;
    int arr[n];
    for(i=0;i<n;i++)
    {
        cin >> arr[i];
        cout << arr[i] << " ";
    }

    cin >> k;
    for(i=0;i<n;i++)
    {
        if(arr[i]==k)
            cout << "Element found at:" << i+1;
    }
    if(i==n)
        cout << "not";
}

Codeforces
1 Magical sticks
#include<iostream>
#include<cmath>
using namespace std;

int main()
{
    int t;
    cin >> t;
    while(t--)
    {
        int n,ans=0;
        cin >> n;
        ans= n/2;
        if(n%2==1)
            ans++;
        cout << ans << endl;
    }
}


